<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fdh.essay.sys.mapper.StudentMapper">

    <select id="selectPageVo" resultType="com.fdh.essay.sys.entity.Student">
        SELECT *
        FROM student
        WHERE age &gt;= #{age}
    </select>
</mapper>
